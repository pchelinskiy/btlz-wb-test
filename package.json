    "main": "app.js",
    "type": "module",
    "scripts": {
        "dev": "nodemon --watch src --ext ts --exec tsx src/app.ts",
        "watch:build": "tsc --build --force --verbose --watch",
        "knex:dev": "tsx src/utils/knex.ts",
        "build": "tsc --allowJs",
        "start": "node dist/app.js",
        "knex": "node dist/utils/knex.js",
        "tsc:check": "tsc --allowJs --noEmit",
        "prettier": "prettier --config .prettierrc.json src/**/*.js --check",
        "prettier-format": "prettier --config .prettierrc.json src/**/*.js --write",
        "eslint": "eslint",
        "eslint-fix": "eslint --fix"
    },
    "imports": {
        "#*": [
            "./dist/*",
            "./src/*"
        ]
    },
    "_moduleAliases": {
        "#": "src"
    },
    "author": "lucard17",
    "license": "ISC",
    "dependencies": {
        "@googleapis/sheets": "^12.0.0",
        "axios": "^1.12.2",
        "commander": "^14.0.1",
        "dotenv": "^17.2.3",
        "googleapis": "^161.0.0",
        "knex": "^3.1.0",
        "log4js": "^6.9.1",
        "p-queue": "^9.0.0",
        "pg": "^8.16.3",
        "pino": "^10.0.0",
        "uuid": "^13.0.0",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@babel/core": "^7.28.4",
        "@babel/preset-env": "^7.28.3",
        "@babel/preset-typescript": "^7.27.1",
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/dateformat": "^5.0.3",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/pg": "^8.15.5",
        "babel-jest": "^30.2.0",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-jsdoc": "^60.8.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-unused-imports": "^4.2.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.10",
